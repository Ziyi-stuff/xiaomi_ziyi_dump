<?xml version="1.0" ?>
<TestCases>
    <Description>Audio</Description>
    <Item>
        <Description>Audio PA</Description>
        <Step index="1">
            <type>exec</type>
            <addr>cat /d/asoc/components | egrep -v soc</addr>
            <operation>exec</operation>
            <value type="show">ERROR</value>
        </Step>
    </Item>

    <Item>
