    <Item>
        <Description>Audio Switch</Description>
        <Step index="1">
            <type>exec</type>
            <addr>cat /d/regmap/1-0042/registers | egrep 00:</addr>
            <operation>exec</operation>
            <value type="show">00: 09</value>
        </Step>
    </Item>

    <Item>
        <Description>vibrator_driver_check</Description>
        <Step index="1">
            <type>exec</type>
            <addr>cat /sys/bus/i2c/drivers/awinic_haptic/*/name</addr>
            <operation>exec</operation>
            <value type="show">0</value>
        </Step>
    </Item>

    <Item>
        <Description>Enable 996log</Description>
        <Step index="1">
            <type>prop</type>
            <addr>debug.offline_log.enable</addr>
            <operation>write</operation>
            <value>true</value>
        </Step>
        <Step index="2">
            <type>prop</type>
            <addr>debug.offline_log.module</addr>
            <operation>write</operation>
            <value>audio</value>
        </Step>
        <Step index="3">
            <type>prop</type>
            <addr>ctl.start</addr>
            <operation>write</operation>
            <value>diag_mdlog_start</value>
        </Step>
    </Item>

    <Item>
        <Description>Disable 996log</Description>
        <Step index="1">
            <type>prop</type>
            <addr>ctl.start</addr>
            <operation>write</operation>
            <value>diag_mdlog_stop</value>
        </Step>
    </Item>

    <Item>
