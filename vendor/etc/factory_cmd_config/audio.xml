        </Step>
        <Step index="2">
            <type>exec</type>
            <addr>spkcal -m</addr>
            <operation>exec</operation>
            <value type="show">0</value>
        </Step>
        <Step index="3">
            <type>file</type>
            <addr>/mnt/vendor/persist/audio</addr>
            <operation>read</operation>
            <value type="show">0</value>
        </Step>
    </Item>

