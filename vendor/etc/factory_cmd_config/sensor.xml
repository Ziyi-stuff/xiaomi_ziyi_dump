    <Item>
        <Description>LsensorTest</Description>
        <Step index="1">
            <type>exec</type>
            <addr>am start -n com.xiaomi.factory.mmi/.test.hardware.screen.MmiAdbBroadcastDisplayCheckActivity</addr>
            <operation delay="800">exec</operation>
            <value type="show">0</value>
        </Step>
        <Step index="2">
            <type>exec</type>
            <addr>settings put system screen_off_timeout 2147483647</addr>
            <operation delay="100">exec</operation>
            <value type="show">0</value>
        </Step>
        <Step index="3">
            <type>exec</type>
            <addr>echo 0 0 > /sys/class/mi_display/disp-DSI-0/disp_param</addr>
            <operation delay="50">exec</operation>
            <value type="show">0</value>
        </Step>
        <Step index="4">
            <type>exec</type>
            <addr>am broadcast -a gray</addr>
            <operation delay="100">exec</operation>
            <value type="show">0</value>
        </Step>
        <Step index="5">
            <type>exec</type>
            <addr>service call SurfaceFlinger 1035 i32 0</addr>
            <operation delay="100">exec</operation>
            <value type="show">0</value>
        </Step>
        <Step index="6">
            <type>file</type>
            <addr>/sys/class/backlight/panel0-backlight/brightness</addr>
            <operation delay="250">write</operation>
            <value>100</value>
        </Step>
        <Step index="7">
            <type>exec</type>
            <addr>test-nusensors -d 200 -s 7 33171007</addr>
            <operation  delay="100">exec</operation>
            <value type="show">0</value>
        </Step>
        <Step index="8">
            <type>file</type>
            <addr>/sys/class/backlight/panel0-backlight/brightness</addr>
            <operation delay="250">write</operation>
            <value>290</value>
        </Step>
        <Step index="9">
            <type>exec</type>
            <addr>test-nusensors -d 200 -s 7 33171007</addr>
            <operation  delay="100">exec</operation>
            <value type="show">0</value>
        </Step>
        <Step index="10">
            <type>file</type>
            <addr>/sys/class/backlight/panel0-backlight/brightness</addr>
            <operation delay="300">write</operation>
            <value>2047</value>
        </Step>
        <Step index="11">
            <type>exec</type>
            <addr>test-nusensors -d 200 -s 7 33171007</addr>
            <operation>exec</operation>
            <value type="show">0</value>
        </Step>
        <Step index="12">
            <type>exec</type>
            <addr>am broadcast -a close</addr>
            <operation delay="100">exec</operation>
            <value type="show">0</value>
        </Step>
    </Item>

