<?xml version="1.0" ?>
<TestCases>
    <Description>Camera</Description>
    <Item>
        <Description>Flashled Up0</Description>
        <Step index="1">
            <type>file</type>
            <addr>/sys/class/leds/led:switch_0/brightness</addr>
            <operation>write</operation>
            <value>0</value>
        </Step>
        <Step index="2">
            <type>file</type>
            <addr>/sys/class/leds/led:torch_0/brightness</addr>
            <operation>write</operation>
            <value>255</value>
        </Step>
       <Step index="3">
            <type>file</type>
            <addr>/sys/class/leds/led:switch_0/brightness</addr>
            <operation>write</operation>
            <value>1</value>
        </Step>
    </Item>

