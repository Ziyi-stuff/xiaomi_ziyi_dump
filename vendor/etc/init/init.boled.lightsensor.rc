on boot
    chown system system /mnt/vendor/persist/sensors/lightSensorCali.json
    #change owner of i2c-flash
    chown system system /sys/kernel/cam-i2c-flash/led_brightness
    chmod 0664 /sys/kernel/cam-i2c-flash/led_brightness
    chown system system /sys/kernel/cam-i2c-flash/led_switch
    chmod 0664 /sys/kernel/cam-i2c-flash/led_switch
